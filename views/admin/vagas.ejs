
<%- include('header') %>
    <h2>Administrador de vagas</h2>
    <div>
        <ul>
            <% vagas.forEach( vaga => { %>
                <li>
                    <%- vaga.titulo %> | 
                    <a href="/admin/vagas/editar/<%- vaga.id %>">Editar</a>
                    <a href="/admin/vagas/delete/<%- vaga.id %>">Excluir</a>                    
                </li>
            <% }) %>
        </ul>
    </div> 
<%- include('footer') %>
